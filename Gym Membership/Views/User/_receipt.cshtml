@model Gym_Membership.Models.Transaction
@using utils = Gym_Membership.Helpers.Utils;
@{
    Layout = null;    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Customer Receipt</title>
    <style>
                /*
             CSS-Tricks Example
             by Chris Coyier
             http://css-tricks.com
        */

                * {
                    margin: 0;
                    padding: 0;
                }

                body {
                    font: 14px/1.4 Georgia, serif;
                }

                #page-wrap {
                    width: 800px;
                    margin: 0 auto;
                }

                textarea {
                    border: 0;
                    font: 14px Georgia, Serif;
                    overflow: hidden;
                    resize: none;
                }

                table {
                    border-collapse: collapse;
                }

                    table td, table th {
                        border: 1px solid black;
                        padding: 5px;
                    }

                #header {
                    height: 15px;
                    width: 100%;
                    margin: 20px 0;
                    background: #222;
                    text-align: center;
                    color: white;
                    font: bold 15px Helvetica, Sans-Serif;
                    /*text-decoration: uppercase;
                    letter-spacing: 20px;*/
                    padding: 8px 0px;
                }

                #address {
                    width: 250px;
                    height: 150px;
                    float: left;
                }

                #customer {
                    overflow: hidden;
                }

                #logo {
                    text-align: right;
                    float: right;
                    position: relative;
                    margin-top: 25px;
                    border: 1px solid #fff;
                    max-width: 540px;
                    max-height: 100px;
                    overflow: hidden;
                }

                #customer-title {
                    font-size: 20px;
                    font-weight: bold;
                    float: left;
                }

                #meta {
                    margin-top: 1px;
                    width: 300px;
                    float: right;
                }

                    #meta td {
                        text-align: right;
                    }

                        #meta td.meta-head {
                            text-align: left;
                            background: #eee;
                        }

                        #meta td textarea {
                            width: 100%;
                            height: 20px;
                            text-align: right;
                        }

                #items {
                    clear: both;
                    width: 100%;
                    margin: 30px 0 0 0;
                    border: 1px solid black;
                }

                    #items th {
                        background: #eee;
                    }

                    #items textarea {
                        width: 80px;
                        height: 50px;
                    }

                    #items tr.item-row td {
                        border: 0;
                        vertical-align: top;
                    }

                    #items td.description {
                        width: 300px;
                        padding-left: 30px;
                    }

                    #items td.item-name {
                        width: 175px;
                    }

                        #items td.description textarea, #items td.item-name textarea {
                            width: 100%;
                        }

                    #items td.total-line {
                        border-right: 0;
                        text-align: right;
                    }

                    #items td.total-value {
                        border-left: 0;
                        padding: 10px;
                        width: 100px;
                    }

                        #items td.total-value textarea {
                            height: 20px;
                            background: none;
                        }

                    #items td.balance {
                        background: #eee;
                    }

                    #items td.blank {
                        border: 0;
                    }

                #terms {
                    text-align: center;
                    margin: 20px 0 0 0;
                }

                    #terms h5 {
                        text-transform: uppercase;
                        font: 13px Helvetica, Sans-Serif;
                        letter-spacing: 1px;
                        border-bottom: 1px solid black;
                        padding: 0 0 8px 0;
                        margin: 0 0 8px 0;
                    }

                    #terms textarea {
                        width: 100%;
                        text-align: center;
                    }

                textarea:hover, textarea:focus, #items td.total-value textarea:hover, #items td.total-value textarea:focus, .delete:hover {
                    background-color: #EEFF88;
                }

                .delete-wpr {
                    position: relative;
                }

                .delete {
                    display: block;
                    color: #000;
                    text-decoration: none;
                    position: absolute;
                    background: #EEEEEE;
                    font-weight: bold;
                    padding: 0px 3px;
                    border: 1px solid;
                    top: -6px;
                    left: -22px;
                    font-family: Verdana;
                    font-size: 12px;
                }

                .rt {
                 float:right;

                }
    </style>

</head>
<body>





    <div id="page-wrap">
        <div id="header">RECEIPT</div>

        <div id="identity">

            <div id="address">
                <b>Space Fitness</b>
                <br />Cercle de Rose Hill
                <br />Boulevard Cowin
                <br />Beau Bassin, Mauritius
                
                <br />
                <br />Phone: 5 443 3300
            </div>

            <div id="logo">

                <img src="data:image/jpeg;base64," alt="LogoFitness.jpg" />
       
                @*<img id="image" src="@Url.Content("~/Images/logo/LogoFitness.jpg")" alt="logo" />*@
            </div>

        </div>

        <div style="clear:both"></div>

        <div id="customer">

            <div id="customer-title">
                @Model.Member.TitleFullname
                <br />  @Model.Member.StreetAddress
                <br />  @Model.Member.Town
            </div>

            <table id="meta">
                <tr>
                    <td class="meta-head">receipt #</td>
                    <td><div>@Model.LastReceipt.ReceiptId</div></td>
                </tr>
                <tr>

                    <td class="meta-head">Date</td>
                    <!-- December 15,2009-->
                    <td><div id="date">@Model.LastReceipt.ReceivedOn.ToString("MMMM dd, yyyy")</div></td>
                </tr>
                <tr>
                    <td class="meta-head">Amount Due</td>


                    <td>

                        @{ var amtPaid = Model.PaidAmount + Model.RegistrationAmount;}
                        
                    <div class="due">Rs @amtPaid.ToString("N2")</div>
                    
                    </td>
                </tr>

            </table>

        </div>

        <table id="items">

            <tr>
                <th>Item</th>
                <th colspan="3">Description</th>
            </tr>


            <tr class="item-row">
                <td class="description">Membership #</td>
                <td colspan="3">@Model.Member.MemberId</td>
            </tr>

            <tr class="item-row">
                <td class="description">Membership Type</td>
                <td colspan="3">@Model.Member.Membership.Name</td>
            </tr>

            <tr class="item-row">
                <td class="description">Period starting from</td>
                <td colspan="3">@Model.StartDate.ToString("dd MMM yyyy")</td>
            </tr>

            <tr class="item-row">
                <td class="description">Period ending on</td>
                <td colspan="3">@Model.EndDate.ToString("dd MMM yyyy")</td>
            </tr>

            <tr class="item-row">
                <td class="description">Received By</td>
                <td colspan="3">@Model.LastReceipt.ReceivedBy.Username</td>
            </tr>
            <tr class="item-row">
                <td class="description">Payment Date</td>
                <td colspan="3">@Model.LastReceipt.ReceivedOn.ToString("dd MMM yyyy")</td>
            </tr>
            <tr class="item-row">
                <td class="description">Payment Method</td>
                <td colspan="3">@Model.LastReceipt.PaymentMethod</td>
            </tr>
            @if (!Model.Member.IsPass)
            {

            <tr class="item-row">
                <td class="description">Next Payment due on</td>
                @{ 
                    var np = Model.Member.PaymentUntilDate.AddDays(1).ToString("dd MMM yyyy");
                    if (Model.UnpaidAmount > 0)
                    {
                        np = utils.GetLastDayOfMonth(Model.LastReceipt.ReceivedOn.AddMonths(1)).ToString("dd MMM yyyy");
                    }
                }
                <td colspan="3">@np</td>
            </tr>

            }


            @if (Model.RegistrationAmount > 0)
            {
                <tr>
                    <td colspan="2" class="blank"> </td>
                    <td class="total-line">Registration Amount</td>
                    <td class="total-value"><div id="">Rs <span class="rt">@Model.RegistrationAmount.ToString("N2")</span></div></td>
                </tr>
            }
            
            <tr>
                <td colspan="2" class="blank"> </td>
                <td class="total-line">Fees</td>
                <td class="total-value">
                
                    @{var fees = Model.PaidAmount + Model.DiscountAmount + Model.UnpaidAmount + Model.WrittenOffAmount;}
                    


                <div id="">Rs <span class="rt">@fees.ToString("N2")</span></div>
                </td>
            </tr>


            @if(Model.RegistrationAmount > 0)
            {


            <tr>
                <td colspan="2" class="blank"> </td>
                <td class="total-line">incl. registration</td>
                <td class="total-value">
                
                    @{var subtot = fees + Model.RegistrationAmount;}
                    


                    <div id="subtotal">Rs <span class="rt">@subtot.ToString("N2")</span></div>
                
                </td>
            </tr>
            }
            @if ( (Model.DiscountAmount + Model.WrittenOffAmount) > 0)
            {
                <tr>
                    <td colspan="2" class="blank"> </td>
                    <td class="total-line">less Discounted</td>
                    <td class="total-value">
                        @{var dsc = Model.DiscountAmount + Model.WrittenOffAmount;}
                    <div id="total">Rs <span class="rt">@dsc.ToString("N2")</span></div>
                    </td>
                </tr>
            }
            @if ((Model.UnpaidAmount) > 0)
            {
                <tr>
                    <td colspan="2" class="blank"> </td>
                    <td class="total-line">less Postponed</td>
                    <td class="total-value">
                        <div id="total">Rs <span class="rt">@Model.UnpaidAmount.ToString("N2")</span></div>
                    </td>
                </tr>
                            }

            <tr>
                <td colspan="2" class="blank"> </td>
                <td class="total-line">Amount Paid</td>
                
                <td class="total-value balance">
                    @*@{ var amtPaid = Model.PaidAmount + Model.RegistrationAmount;}*@
                    
                <div class="due">Rs <span class="rt">@amtPaid.ToString("N2")</span></div>
                </td>
            </tr>

        </table>

        <div id="terms">
            @*<h5>Terms</h5>
            <div>NET 30 Days. Finance Charge of 1.5% will be made on unpaid balances after 30 days.</div>*@
        </div>

    </div>


</body>
</html>
